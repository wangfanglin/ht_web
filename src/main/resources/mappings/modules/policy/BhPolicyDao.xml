	<select id="getByPolicyinfoid" resultType="BhPolicy">
		SELECT ${sqlMap.column.toSql()}
		FROM ${sqlMap.table.toSql()}
		<where>
			policyinfoid = #{policyinfoid}
		</where>
		ORDER BY ${sqlMap.order.toSql()}
		limit 1
	</select>
	
